{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/partials/header/header.tsx", "app/partials/header.js", "next-app-template/components/icons.tsx", "app/partials/header.tsx", "app/pages/api/sendEmail.js", "app/api/test.js", "app/pages/api/mail.js", "app/pages/api/mail", "app/pages/api/test"],
  "exclude": ["node_modules"]
}
